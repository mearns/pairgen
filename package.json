{
  "name": "pairgen",
  "version": "1.0.0",
  "description": "",
  "main": "dest/src/main.js",
  "bin": "dest/src/main.js",
  "scripts": {
    "test": "nyc mocha ./dest/test/src/**/*.spec.js",
    "compile": "node build-tools/transpiler src test package.json",
    "start": "node . server --host localhost --port 8080",
    "debug-server": "node --nolazy --inspect-brk=9229 . server --host localhost --port 8089",
    "debug-cli": "node --nolazy --inspect-brk=9229 . cli"
  },
  "author": "",
  "license": "ISC",
  "nyc": {
    "reporter": [
      "text",
      "lcov"
    ],
    "include": [
      "src/**"
    ],
    "all": true,
    "check-coverage": true,
    "statements": 25,
    "branches": 25,
    "functions": 25,
    "lines": 25
  },
  "dependencies": {
    "express": "4.16.3",
    "express-graphql": "0.6.12",
    "graphql": "0.13.2",
    "graphql-type-long": "0.1.1",
    "http-status-codes": "1.3.0",
    "humanize-duration": "3.15.0",
    "lodash.pick": "4.4.0",
    "moment": "2.22.2",
    "morgan": "1.9.0",
    "source-map-support": "0.5.6",
    "timestring": "5.0.1",
    "yargs": "12.0.1"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "fs-extra": "6.0.1",
    "fs-walk": "0.0.2",
    "mkdirp": "0.5.1",
    "mocha": "5.2.0",
    "mz": "2.7.0",
    "nyc": "12.0.2",
    "source-map": "0.7.3",
    "standard": "11.0.1"
  }
}
